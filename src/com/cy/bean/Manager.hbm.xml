<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cy.bean.Manager" table="manager">
		<id name="managerid" column="managerid">
			<generator class="native"></generator>
		</id>
		<property name="managername" column="managername"></property>
		<property name="passwd" column="passwd"></property>
		<property name="mstate" column="mstate"></property>
	</class>
</hibernate-mapping>